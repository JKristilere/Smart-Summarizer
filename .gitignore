# Create .gitignore file
cat > .gitignore << 'EOF'
# Environment
.env
*.env
.env.local

# Python
__pycache__/
*.py[cod]
*.egg-info/
.venv/
venv/
uv.lock

# Data
/data/
/notebooks/
*.db
*.sqlite

# IDE
.vscode/
.idea/
*.swp

# Logs
*.log

# OS
.DS_Store
Thumbs.db
EOF

# Add to .gitignore
echo ".streamlit/secrets.toml" >> .gitignore


git add .gitignore
git commit -m "Add .gitignore"
git push